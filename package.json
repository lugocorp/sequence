    "ecosystem:cordova"
  ],
  "author": "Alex Lugo",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=14.20.1",
    "nvm": ">=8.19.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "cordova": "^11.0.0",
    "cordova-plugin-file": "^6.0.2",
    "eslint": "^8.6.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "cordova-android": "^8.1.0"
