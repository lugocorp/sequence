{
  "name": "net.lugocorp.sequence",
  "displayName": "Sequence Dungeons",
  "version": "1.0.0",
  "description": "A retro RPG rougelite game.",
  "scripts": {
    "compile": "npx tsc",
    "lint": "npx eslint src",
    "serve": "npm run compile && npx cordova serve 3000",
    "start": "npx nodemon -w www -w src -i www/src -e \"html css js ts\" --exec npm run serve"
  },
  "keywords": [
    "ecosystem:cordova"
  ],
  "author": "Alex Lugo",
  "license": "Apache-2.0",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "cordova": "^11.0.0",
    "eslint": "^8.6.0",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.4"
  }
}
